{
  "name": "nestjs-mysql-starter",
  "version": "1.0.0",
  "description": "A production-ready NestJS boilerplate with authentication, role-based access, file uploads, and queue processing",
  "author": {
    "name": "Your Name",
    "email": "your.email@domain.com"
  },
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "mig:run": "nest build && typeorm migration:run -d dist/ormconfig.js --transaction each",
    "mig:revert": "nest build && typeorm migration:revert -d dist/ormconfig.js --transaction each",
    "migrate:gen": "nest build && typeorm migration:generate -d dist/ormconfig.js",
    "migrate:empty": "typeorm migration:create "
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@aws-sdk/s3-request-presigner": "^3.812.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.5",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "@sendgrid/mail": "^8.1.4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/xlsx": "^0.0.35",
    "adm-zip": "^0.5.16",
    "aws-sdk": "^2.1692.0",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bull": "^4.16.5",
    "chokidar": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "client-ip": "^1.0.0",
    "csv-parse": "^5.6.0",
    "ejs": "^3.1.10",
    "exceljs": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "loadash": "^1.0.0",
    "mime-types": "^3.0.1",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "nanoid": "^5.1.5",
    "nest": "^0.1.6",
    "nestjs-request-context": "^3.0.0",
    "pdfkit": "^0.15.1",
    "postmark": "^4.0.5",
    "puppeteer": "^24.10.0",
    "reflect-metadata": "^0.2.0",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.20",
    "typeorm-transactional": "^0.5.0",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/adm-zip": "^0.5.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.10",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/pdfkit": "^0.13.6",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
